script(src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js", integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN", crossorigin="anonymous")
script(src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js", integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV", crossorigin="anonymous")      

script.
  const connect_timeout = 5000;
  const success_code = 200;
  var block_list = [-500, -400, -300];
  var youtube_link_prefix = /^(?:https?:\/\/)?(?:m\.|www\.)?(?:youtu\.be\/|youtube\.com\/(?:embed\/|v\/|watch\?v=|watch\?.+&v=))((\w|-){11})(?:\S+)?$/;

  var startLoading = function () {
    $("#contentPage").addClass("loading-page");
  }
  var endLoading = function () {
    $("#contentPage").removeClass("loading-page");
  }
  var callOnClick = function (button, action) {
    $(document).off("click", button).on("click", button, function () {
      action(this);
    });
  }

  var goToByScroll = function(id) {
    // Scroll
    $('html,body').animate({
        scrollTop: $(id).offset().top
    }, 'slow');
  }